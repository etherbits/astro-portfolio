{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/api\\/send-mail$",
			"dest": "api/send-mail.ts"
		},
		{
			"src": "^\\/([^/]+?)\\/?$",
			"dest": "[lang]/index.astro"
		},
		{
			"src": "^\\/([^/]+?)\\/contact-me\\/?$",
			"dest": "[lang]/contact-me/index.astro"
		},
		{
			"src": "^\\/([^/]+?)\\/projects\\/?$",
			"dest": "[lang]/projects/index.astro"
		},
		{
			"src": "^\\/([^/]+?)\\/journey\\/?$",
			"dest": "[lang]/journey/index.astro"
		}
	]
}